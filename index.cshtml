//Razor view file that contains the HTML markup and C# code used to render the telemetry data to the user's browser. 

@model List<TelemetryModel>

<table>
    <tr>
        <th>ConnectionDeviceId</th>
        <th>EnqueuedTime</th>
        <th>Altitude</th>
        <th>Latitude</th>
        <th>Longitude</th>
        <th>Pitch</th>
        <th>Roll</th>
        <th>Yaw</th>
    </tr>
    @foreach (var telemetry in Model)
    {
        <tr>
            <td>@telemetry.ConnectionDeviceId</td>
            <td>@telemetry.EnqueuedTime</td>
            <td>@telemetry.Altitude</td>
            <td>@telemetry.Latitude</td>
            <td>@telemetry.Longitude</td>
            <td>@telemetry.Pitch</td>
            <td>@telemetry.Roll</td>
            <td>@telemetry.Yaw</td>
        </tr>
    }
</table>
